<div class='container'>
  <div class='row'>
    <div class='col-lg-4 offset-lg-1 mb-5'>
      <h3 class="alert alert-info text-center">会員一覧</h3>
    </div>
  </div>
  <div class="text-center">
    <div class="col-lg-12">
      <table class='table'>
        <thead class="thead-light">
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= user.id %></td>
              <td>
                <%= link_to master_user_path(user) do %>
                  <%= user.first_name %> <%= user.last_name %>
                <% end %>
              </td>
              <td><%= user.email %></td>
              <td><%= user.deleted_user ? '有効' : '退会' %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  <div>
</div>